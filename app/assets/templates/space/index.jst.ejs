
<div class="col-xs-5 map-container">
<div id='map-canvas'>
</div>
</div>

<div class='col-xs-7 container-full index-box'>
  <% spaces.each(function(space){ %>
      <div class='col-xs-6 image-preview'>
        <a href='#/spaces/<%= space.id %>'>
            <img class='index-image' src='<%= space.escape("photo_url") %>'>
            <img class='avatar-image img-circle' src='<%= space.escape("user_avatar") %>'>
            <div class='price-tag'><%= space.escape('price') + 'g' %></div>
            <h2 class='space-title'><%= space.escape('title') %></h2>
        </a>
      </div>
    <% }); %>
</div>
